generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Provider {
  id           String   @id @default(cuid())
  email        String   @unique
  businessName String
  city         String
  state        String
  zip          String?
  lat          Float
  lng          Float
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt

  @@index([lat, lng])
}
